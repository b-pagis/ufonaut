{
  "compilerOptions": {
    "target": "ES2019",
    "lib": ["ES2019"],
    "module": "commonjs",
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "removeComments": true,
    "declaration": false,
    "sourceMap":true
  },
  "exclude": [
    "**/node_modules/*",
    "lib"
  ]
}